package com.myteam.swimmingpool;

import java.lang.Number;
import com.myteam.swimmingpool.SeasonType;
import com.myteam.swimmingpool.Period;

rule "InsertEnumRule"
    dialect "java"
    salience 100
    when
        InsertPeriod(eVal : enumValue!=null)
    then
        Period p = new Period();
        p.setSeasonType(SeasonType.valueOf(eVal));
        insert(p);
end    

