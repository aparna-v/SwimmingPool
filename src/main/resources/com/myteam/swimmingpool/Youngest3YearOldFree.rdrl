package com.myteam.swimmingpool;

import java.lang.Number;
import java.util.List;

rule "Youngest3YearOldFree"
	dialect "mvel"
	when
		List( size() > 3 ) from collect ( 
Person( age > 3 )) 
		nn : Number( ) from accumulate ( Person( aage : age > 3 ),
			min(aage)) 
		aPerson : Person( age == ( nn ) , standardPrice != ( 1.0B ) )
	then
end
